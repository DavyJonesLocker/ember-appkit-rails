<style>

  body {
    margin: 0 0 25px 0;
    padding: 0;
    background-color: #f0f0f0;
    font-family: "Lucida Grande", "Bitstream Vera Sans", "Verdana";
    font-size: 13px;
    color: #333;
  }

  h1 {
    font-size: 28px;
    color: #000;
  }

  a  {
    color: #03c;
  }

  a:hover {
    background-color: #03c;
    color: white;
    text-decoration: none;
  }

  #page {
    background-color: #f0f0f0;
    width: 760px;
    margin: 0 auto;
    overflow: auto;
  }

  #content {
    margin-bottom: 2em;
    float: left;
    background-color: white;
    border: 3px solid #aaa;
    border-top: none;
    padding: 25px;
    width: 505px;
    min-height: 550px;
  }

  #sidebar {
    float: right;
    width: 175px;
  }

  #sidebar ul {
    padding-left: 0;
  }

  #sidebar ul h3 {
    margin-top: 25px;
    font-size: 16px;
    padding-bottom: 16px;
    border-bottom: 1px solid #ccc;
  }

  #sidebar li {
    list-style-type: none;
  }

  #sidebar ul.links li {
    margin-bottom: 5px;
  }

  footer {
    width: 560px;
    text-align: center;
    }

  svg {
    float: left;
    position: absolute;
    left: 0;
  }

  header {
    position: relative;
    height: 64px;
  }

  header h1, header h2 {
    margin: 0
  }

  header h2 {
    color: #888;
    font-weight: normal;
    font-size: 16px;
  }

  header, #getting-started {
    padding-left: 75px;
    padding-right: 30px;
  }

  #getting-started {
    border-top: 1px solid #ccc;
    margin-top: 25px;
    padding-top: 15px;
  }

  #getting-started h1 {
    margin: 0;
    font-size: 20px;
  }

  #getting-started h2 {
    margin: 0;
    font-size: 14px;
    font-weight: normal;
    color: #333;
    margin-bottom: 25px;
  }

  #getting-started ol {
    margin-left: 0;
    padding-left: 0;
  }

  #getting-started li {
    font-size: 18px;
    color: #888;
    margin-bottom: 25px;
  }

  #getting-started li h2 {
    margin: 0;
    font-weight: normal;
    font-size: 18px;
    color: #333;
  }

  #getting-started li p {
    color: #555;
    font-size: 13px;
  }
</style>

